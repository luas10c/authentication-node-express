generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id @default(uuid())
  name     String?
  email    String    @unique
  password String?
  phone    String?   @unique
  birthday DateTime?

  @@map("users")
}
